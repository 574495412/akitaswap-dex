rcle } from 'react-feather';\nimport { ImageProps } from 'rebass';\n\nconst BAD_SRCS: { [tokenAddress: string]: true } = {};\n\nexport interface LogoProps extends Pick<ImageProps, 'style' | 'alt' | 'className'> {\n  srcs: string[];\n}\n\n/**\n * Renders an image by sequentially trying a list of URIs, and then eventually a fallback triangle alert\n */\nexport default function Logo({ srcs, alt, ...rest }: LogoProps) {\n  const [, refresh] = useState<number>(0);\n\n  const src: string | undefined = srcs.find((src) => !BAD_SRCS[src]);\n\n  if (src) {\n    return (\n      <img\n        {...rest}\n        alt={alt}\n        src={src}\n        onError={() => {\n          if (src) BAD_SRCS[src] = true;\n          refresh((i) => i + 1);\n        }}\n      />\n    );\n  }\n\n  return <HelpCircle {...rest} />;\n}\n","C:\\Users\\HOME\\Desktop\\akitaswap-dex\\frontend\\src\\hooks\\useTheme.ts",["1008"],"import { ThemeContext } from 'styled-components';\nimport { useContext } from 'react';\n\nexport default function useTheme() {\n  return useContext(ThemeContext);\n}\n","C:\\Users\\HOME\\Desktop\\akitaswap-dex\\frontend\\src\\hooks\\useToggle.ts",["1009"],"import { useCallback, useState } from 'react';\n\nexport default function useToggle(initialState = false): [boolean, () => void] {\n  const [state, setState] = useState(initialState);\n  const toggle = useCallback(() => setState((state) => !state), []);\n  return [state, toggle];\n}\n","C:\\Users\\HOME\\Desktop\\akitaswap-dex\\frontend\\src\\hooks\\useOnClickOutside.tsx",["1010","1011"],"import { RefObject, useEffect, useRef } from 'react';\n\nexport function useOnClickOutside<T extends HTMLElement>(\n  node: RefObject<T | undefined>,\n  handler: undefined | (() => void)\n) {\n  const handlerRef = useRef<undefined | (() => void)>(handler);\n  useEffect(() => {\n    handlerRef.current = handler;\n  }, [handler]);\n\n  useEffect(() => {\n    const handleClickOutside = (e: MouseEvent) => {\n      if (node.current?.contains(e.targe